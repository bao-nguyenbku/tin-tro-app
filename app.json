{
  "expo": {
    "name": "tin-tro",
    "slug": "tin-tro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./public/splash-2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.baonguyen.tintro",
      "infoPlist": {
        "NSAllowsArbitraryLoads": {
            "NSAllowsArbitraryLoads": true
        }
    }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.baonguyen.tintro",
      "versionCode": 1
    },
    "web": {
      "favicon": "./public/favicon.png",
      "bundler": "metro"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "flipper": true
          }
        }
      ]
    ]
  }
}
